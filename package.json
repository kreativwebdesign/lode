{
  "name": "lode",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "repository": "https://github.com/kreativwebdesign/lode.git",
  "scripts": {
    "setup-benchmark": "cd ./benchmark && yarn setup",
    "setup-cli": "cd ./cli && yarn setup",
    "setup-lode-three": "cd ./lode-three && yarn setup",
    "setup-demo": "cd ./demo && yarn setup",
    "setup-lode-ui": "cd ./lode-ui && yarn setup",
    "setup-plotter": "cd ./plotter && yarn setup",
    "setup": "yarn setup-benchmark && yarn setup-cli && yarn setup-lode-three && yarn setup-demo && yarn setup-lode-ui && yarn setup-plotter",
    "pdf-readme-benchmark": "pandoc -f markdown -t pdf --listings -H ./documentation/report/scripts/listings-setup.tex -o ./documentation/report/ressources/packages/benchmark.pdf ./benchmark/README.md",
    "pdf-readme-cli": "pandoc -f markdown -t pdf --listings -H ./documentation/report/scripts/listings-setup.tex -o ./documentation/report/ressources/packages/cli.pdf ./cli/README.md",
    "pdf-readme-lode-three": "pandoc -f markdown -t pdf --listings -H ./documentation/report/scripts/listings-setup.tex -o ./documentation/report/ressources/packages/lode-three.pdf ./lode-three/README.md",
    "pdf-readme-demo": "pandoc -f markdown -t pdf --listings -H ./documentation/report/scripts/listings-setup.tex -o ./documentation/report/ressources/packages/demo.pdf ./demo/README.md",
    "pdf-readme-lode-ui": "pandoc -f markdown -t pdf --listings -H ./documentation/report/scripts/listings-setup.tex -o ./documentation/report/ressources/packages/lode-ui.pdf ./lode-ui/README.md",
    "pdf-readme-plotter": "pandoc -f markdown -t pdf --listings -H ./documentation/report/scripts/listings-setup.tex -o ./documentation/report/ressources/packages/plotter.pdf ./plotter/README.md",
    "pdf-readme": "yarn pdf-readme-benchmark && yarn pdf-readme-cli && yarn pdf-readme-lode-three && yarn pdf-readme-demo && yarn pdf-readme-lode-ui && yarn pdf-readme-plotter"
  }
}
